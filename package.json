{
  "name": "@dome-protocol/sdk",
  "version": "0.2.1-rc.1",
  "description": "The frontend sdk to operate with dome onchain entities",
  "repository": {
    "type": "git",
    "url": "https://github.com/Polynom-Labs/dome-sdk"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/core/index.js",
        "types": "./dist/core/types/index.d.ts"
      }
    },
    "./react": {
      "import": {
        "default": "./dist/react/index.js",
        "types": "./dist/react/types/react/index.d.ts"
      }
    }
  },
  "files": [
    "!dist/react/types",
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf ./dist && rollup --config rollup.config.ts --configPlugin typescript",
    "test": "jest ./src/**/*.test.ts",
    "test:e2e": "jest ./e2e/**/*.test.ts",
    "test:types": "tsc --noEmit"
  },
  "keywords": [
    "Dome",
    "SDK",
    "Miden",
    "ZK"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "viem": "^2.34.0",
    "wagmi": "^2.16.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "29.5.6",
    "@types/react": "^19.2.2",
    "@uniswap/sdk-core": "^7.7.2",
    "@uniswap/swap-router-contracts": "^1.3.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v2-sdk": "^4.16.0",
    "@uniswap/v3-periphery": "^1.4.4",
    "@uniswap/v3-sdk": "^3.25.2",
    "dotenv-cli": "^10.0.0",
    "jest": "^30.1.2",
    "react": "^18.3.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.0",
    "testcontainers": "^11.7.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  }
}
